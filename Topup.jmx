            <stringProp name="script">if(prev.getResponseCode() == &quot;200&quot;){
    String res=prev.getResponseDataAsString();
	res=res.replace(&quot;\&quot;&quot;, &quot;&quot;);
	res=res.replace(&quot;\&quot;&quot;, &quot;&quot;);
    String txnid=res.substring(res.indexOf(&quot;gle_txn_id&quot;),res.indexOf(&quot;user_a_msg&quot;));
    txnid=txnid.replace(&quot;gle_txn_id:&quot;,&quot;&quot;);
 	txnid=txnid.substring(0, txnid.length() - 1);
    //vars.put(&quot;response&quot;, serial);
    File file = new File(&quot;${__V(csv_file_name)}.csv&quot;);
    FileWriter fstream = new FileWriter(file,true);
	BufferedReader br = new BufferedReader(new FileReader(file)) 
    if (br.readLine() == null) {
    	fstream.write(&quot;    GLE Transaction ID    \n&quot;);
    }
    fstream.write(txnid+&quot;\n&quot;);
    fstream.close();    
}</stringProp>
