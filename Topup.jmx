            <stringProp name="script">
    String res=prev.getResponseDataAsString();
	res=res.replace(&quot;\&quot;&quot;, &quot;&quot;);
	res=res.replace(&quot;\&quot;&quot;, &quot;&quot;);
	
    String user=res.substring(res.indexOf(&quot;user_id&quot;),res.indexOf(&quot;user_status&quot;));
	user=user.replace(&quot;user_id:&quot;,&quot;&quot;);
	user=user.substring(0, user.length() - 1);
	
     String session_id=res.substring(res.indexOf(&quot;session_id&quot;),res.indexOf(&quot;skey&quot;));
     session_id=session_id.replace(&quot;session_id:&quot;,&quot;&quot;);
	session_id=session_id.substring(0, session_id.length() - 1);
	
	String skey=res.substring(res.indexOf(&quot;skey&quot;),res.indexOf(&quot;full_name&quot;));
     skey=skey.replace(&quot;skey:&quot;,&quot;&quot;);
     skey=skey.substring(0, skey.length() - 1);

 	props.put(&quot;userId&quot;, user);
	props.put(&quot;sessionId&quot;,session_id);
	props.put(&quot;skey&quot;,skey);</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
